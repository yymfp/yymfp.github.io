<ul class="am-pagination">
    <% if (page.prev) { %>
    	<li class="am-pagination-prev">
   		<a class="pull-left" href="<%- url_for(page.prev.path) %>" title="<%- page.prev.title %>">
      		&laquo;上一篇 
		</a>
		</li>
	<% } %>
	<% if (page.next) { %>
		<li class="am-pagination-next">
		<a class="pull-right" href="<%- url_for(page.next.path) %>" title="<%- page.next.title %>">
			下一篇&raquo;
		</a>
		</li>
	 <% } %>
 </ul>
        